{
  "name": "A browser action with a popup that get information from forum",
  "description": "forum data handler",
  "version": "1.0",
  "permissions": [
    "downloads",
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "forum data handler",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["inject_files/page_handler.js"]
}
